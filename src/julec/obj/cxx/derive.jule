use std::jule::sema::{Struct}

struct DeriveCoder {}

impl DeriveCoder {
    static fn fn_decl_clone(&s: &Struct): str {
        let mut obj = TypeCoder.from_struct(s)
        obj += " clone(void) const "
        ret obj
    }

    static fn fn_def_clone(&s: &Struct): str {
        let mut obj = TypeCoder.from_struct(s)
        obj += " " + obj
        obj += "::clone(void) const "
        ret obj
    }
}
