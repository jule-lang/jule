use std::jule::sema::{Struct}

struct deriveCoder {}

impl deriveCoder {
    static fn cloneFuncDecl(&s: &Struct): str {
        let mut obj = typeCoder.structure(s)
        obj += " clone(void) const "
        ret obj
    }

    static fn cloneFuncDef(&s: &Struct): str {
        let mut obj = typeCoder.structure(s)
        obj += " " + obj
        obj += "::clone(void) const "
        ret obj
    }
}