// See [std::strings::{Compare}] function for documentation.
#export "__jule_compareStr"
fn compareStr(&a: str, &b: str): int {
	mut l := len(a)
	if len(b) < l {
		l = len(b)
	}
	mut i := 0
	for i < l; i++ {
		c1, c2 := a[i], b[i]
		if c1 < c2 {
			ret -1
		}
		if c1 > c2 {
			ret +1
		}
	}
	if len(a) < len(b) {
		ret -1
	}
	if len(a) > len(b) {
		ret +1
	}
	ret 0
}