cpp use "unsafe.hpp"

cpp fn str_to_byte_slice(s: str): []byte

// Returns byte slice that uses string's allocation.
// Does not performs deallocation.
fn stobs(&s: str): []byte {
    ret cpp.str_to_byte_slice(s)
}
