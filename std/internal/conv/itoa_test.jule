#build test

use std::testing::{T}

struct itoaCase {
    i: int
    s: str
}

static casesItoa: []itoaCase = [
    {-983, "-983"},
    {-873, "-873"},
    {98837792362, "98837792362"},
    {-333444, "-333444"},
    {0, "0"},
]

struct utoaCase {
    i: uint
    s: str
}

static casesUtoa: []utoaCase = [
    {983, "983"},
    {873, "873"},
    {98837792362, "98837792362"},
    {333444, "333444"},
    {0, "0"},
]

#test
fn testUtoa(t: &T) {
    for _, case in casesUtoa {
        s := Utoa(case.i)
        if s != case.s {
            t.Errorf("expected {} for {}, found {}", case.s, case.i, s)
        }
    }
}