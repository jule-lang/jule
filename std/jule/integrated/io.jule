cpp unsafe fn puts(s: *Char): Int

// Calls C's puts function.
// Returns non-negative integer if success, returns EOF if not.
pub fn puts(s: str): int {
    ret unsafe { int(cpp.puts((*Char)(&s[0]))) }
}
