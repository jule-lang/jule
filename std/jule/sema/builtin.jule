    }
}

fn findBuiltinDefStdMem(ident: str): any {
    match ident {
    | "SizeOf":
        static mut f = &FnIns{caller: builtinCallerStdMemSizeOf}
        ret f
    | "AlignOf":
        static mut f = &FnIns{caller: builtinCallerStdMemAlignOf}
