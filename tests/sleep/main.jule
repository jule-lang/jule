use "std/math/rand"
use "std/time"

fn main() {
	let mut rand = rand::Rand.New(time::Time.Now().Unix())
	let mut i = 0
	for i < 10; i++ {
		let dur = time::Duration.Millisecond * rand.Nextn63(1000)
		out("Sleeping for ")
		out(time::Duration.Milliseconds(dur))
		outln("ms")
		time::Sleep(dur)
	}
	outln("Done!")
}