fn quicksort(mut s: []int) {
	if len(s) <= 1 {
		ret
	}

	let mut i = -1
	let last = s[len(s)-1]
	for j in s {
		if s[j] <= last {
			i++
			s[i], s[j] = s[j], s[i]
		}
	}

	quicksort(s[:i])
	quicksort(s[i+1:])
}

fn main() {
	let mut mySlice = [1, 9, -2, 25, -24, 4623, 0, -1, 0xFD2]
	println(mySlice)
	quicksort(mySlice)
	println(mySlice)
}