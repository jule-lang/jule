fn main() {
	let mut m: map[i32]str = {
		0: "The",
		1: "Jule",
		2: "Programming",
		3: "Language",
	}
	for key, value in m {
		out(key)
		out(": ")
		outln(value)
	}
	outln(len(m))
	let mut ok = false
	_, ok = m[0]
	outln(ok)
	_, ok = m[10]
	outln(ok)
	delete(m, 3)
	delete(m)
	outln(len(m))
}